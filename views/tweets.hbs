<div id="tweets-header"><h1>Tweets</h1></div>

<div id="tweets-container" class="container"> 
  <div class="row">
    <div class="col-sm-4 timeline-column">
        <div class="account-title">Tweets by @{{userAccountATweets.0.screenName}}</div>
        {{#each userAccountATweets}}
            <blockquote class="twitter-tweet Tweet h-entry js-tweetIdInfo subject expanded is-deciderHtmlWhitespace" cite="https://twitter.com/{{this.screenName}}/status/{{this.id}}" data-tweet-id="{{this.id}}" data-scribe="section:subject">
                {{#if retweeted}}
                    <p class="retweet-indicator">{{this.screenName}} retweeted</p>
                {{/if}}
                <div class="Tweet-header u-cf">
                    <div class="Tweet-brand u-floatRight">
                        <a href="https://twitter.com/{{this.screenName}}/status/{{this.id}}" data-scribe="element:logo" target="_blank">
                            <span class="FollowButton-bird">
                                <div class="Icon Icon--twitter" aria-label="View on Twitter" title="View on Twitter" role="presentation">
                                Follow</div>
                            </span>
                        </a>
                    </div>

                    <div class="TweetAuthor js-inViewportScribingTarget js-aBScribingTarget" data-scribe="component:author">
                        <a class="TweetAuthor-link Identity u-linkBlend" data-scribe="element:user_link" href="https://twitter.com/{{this.screenName}}" aria-label="{{this.screenName}} (screen name: {{this.screenName}})" target="_blank">
                            <span class="TweetAuthor-avatar Identity-avatar">
                                <img class="Avatar Avatar--edge" data-scribe="element:avatar" data-src-2x="{{this.profileImageUrl}}" alt="" data-src-1x="{{this.profileImageUrl}}" src="{{this.profileImageUrl}}">
                            </span>
                            <span class="TweetAuthor-decoratedName">
                                <span class="TweetAuthor-name TweetAuthor-name--flex Identity-name customisable-highlight" title="{{this.screenName}}" data-scribe="element:name">{{this.screenName}}</span>
                            </span>
                            <span class="TweetAuthor-screenName Identity-screenName" title="@{{this.screenName}}" data-scribe="element:screen_name" dir="ltr">@{{this.screenName}}</span>
                        </a>
                    </div>
                </div>

                <div class="Tweet-body e-entry-content" data-scribe="component:tweet">
                
                <div class="u-hiddenVisually js-inViewportScribingTarget"></div>
                <p class="Tweet-text e-entry-title" lang="en" dir="ltr">
                    {{{this.tweetText}}}
                </p>

                {{#if this.summaryUrl}}
                <a href="{{this.summaryUrl}}" target="_blank">
                    <div class="summary-card">
                        {{!-- <p>summary-site: {{this.summarySite}}</p> --}}
                        <p>{{this.summaryTitle}}</p>
                        <p>{{this.summaryDescription}}</p>
                    </div>
                </a>
                {{/if}}

                <div class="Tweet-metadata dateline">      
                    <a class="u-linkBlend u-url customisable-highlight long-permalink" data-datetime="{{this.createdAt}}" data-scribe="element:full_timestamp" href="https://twitter.com/{{this.screenName}}/status/{{this.id}}" target="_blank">
                        <time class="dt-updated" datetime="{{this.createdAt}}" pubdate="" title="Time posted: {{this.createdAt}}">{{this.createdAt}}</time>
                    </a>
                </div>

                <div class="u-hiddenVisually js-aBScribingTarget"></div>

                <ul class="Tweet-actions" data-scribe="component:actions" role="menu" aria-label="Tweet actions">
                    <li class="Tweet-action">
                        <a class="TweetAction TweetAction--replyEdge web-intent" href="https://twitter.com/intent/tweet?in_reply_to={{this.id}}" data-scribe="element:reply" target="_blank">
                            <div class="Icon Icon--reply TweetAction-icon Icon--replyEdge" aria-label="Reply" title="Reply" role="img"></div>
                            <span class="TweetAction-stat" data-scribe="element:reply_count" aria-hidden="true"></span>
                            <span class="u-hiddenVisually"> Reply</span>
                        </a>
                    </li>
                    <li class="Tweet-action">
                        <a class="TweetAction TweetAction--retweetEdge web-intent" href="https://twitter.com/intent/retweet?tweet_id={{this.id}}" data-scribe="element:retweet" target="_blank">
                            <div class="Icon Icon--retweet TweetAction-icon Icon--retweetEdge" aria-label="Retweet" title="Retweet" role="img"></div>
                            <span class="TweetAction-stat" data-scribe="element:retweet_count" aria-hidden="true"></span>
                            <span class="u-hiddenVisually"> Retweet</span>
                        </a>
                    </li>
                    <li class="Tweet-action">
                        <a class="TweetAction TweetAction--heartEdge web-intent" href="https://twitter.com/intent/like?tweet_id={{this.id}}" data-scribe="element:heart" target="_blank">
                            <div class="Icon Icon--heart TweetAction-icon Icon--heartEdge" aria-label="Like" title="Like" role="img"></div>
                            <span class="TweetAction-stat" data-scribe="element:heart_count" aria-hidden="true"></span>
                            <span class="u-hiddenVisually"> Like</span>
                        </a>
                    </li>
                </ul>

                </div>
            </blockquote>
        {{/each}}
    </div>
